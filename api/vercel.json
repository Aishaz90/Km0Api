{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/images/(.*)",
            "dest": "/../images/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}